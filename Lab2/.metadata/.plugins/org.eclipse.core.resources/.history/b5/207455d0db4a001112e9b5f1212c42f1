import java.util.Random;

public class PhoneNumber {

	
	public static void main(String[] args) {
		
		int areaCode;
		
		Random randomInt = new Random();
		
		areaCode = randomInt.nextInt(8) + randomInt.nextInt(8) + randomInt.nextInt(8);
		
		/*Generates Random phone number 
		with no 8s or 9s in Area Code, 
		and nothing greater than 742 in next three digits
		*/
		System.out.println(areaCode/*randomInt.nextInt(8)
							randomInt.nextInt(8)
							randomInt.nextInt(8)*/
							+ "-"
							+ randomInt.nextInt(743)
							+ "-"
							+ randomInt.nextInt(10000));
		

	}

}
