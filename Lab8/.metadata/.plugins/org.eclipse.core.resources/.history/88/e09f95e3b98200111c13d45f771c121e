public class LLBank {
	
	final int MAX = 30; //max number of customers
	final double INTEREST = 1.03; //3% interest
	static int count = 0;
	LLBankCustomer[] customers;
	
	
	//Constructor
	public LLBank(long account, String owner, String pNum,  double initial){
		
		System.out.println("Creating new Account.");
		customers = new LLBankCustomer[count];
		
		customers[count].setAcctNum(account);
		customers[count].setName(owner);
		customers[count].setPhoneNum(pNum);
		customers[count].setBalance(initial);
		
		count++;
	}//end of Constructor
	
	
	public void withdrawl(){
		
	}//end of withdrawl method
	
	public void deposit(){
		
	}//end of deposit method
	
	@Override
	public String toString(){
		return "Enter Something Here....";
	}//end of toString override method

}//end of LLBank Class
