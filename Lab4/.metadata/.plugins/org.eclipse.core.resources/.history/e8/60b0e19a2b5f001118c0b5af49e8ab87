import java.util.Random;

public class BoxTest {

	public static void main(String[] args) {
		// TODO Instantiate and update several Box objects
		
		
		Random randomInt = new Random();//makes a random Int that is used for the box dimensions
		
		Box box1 = new Box((randomInt.nextInt(10)+1), (randomInt.nextInt(10)+1), (randomInt.nextInt(10)+1));//makes the box dimensons 1-10 randomly
		Box box2 = new Box((randomInt.nextInt(10)+1), (randomInt.nextInt(10)+1), (randomInt.nextInt(10)+1));
		Box box3 = new Box((randomInt.nextInt(10)+1), (randomInt.nextInt(10)+1), (randomInt.nextInt(10)+1));
		
		//output the current box dimensions before we change them
		System.out.println(box1);
		System.out.println(box2);
		System.out.println(box3);
		
		//change the box dimensions
		
	}//end of main

}//end of Class BoxTest
